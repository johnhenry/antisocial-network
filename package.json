{
  "name": "antisocial-network",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deno:run": "deno run --importmap=import_map.json --unstable-sloppy-imports --allow-env --allow-read --allow-net",
    "env:vars": "npm run deno:run -s src/script/show-config.ts",
    "minio": "minio server ./.minio",
    "surreal": "surreal start file://./.surreal -A --auth --user root --pass root",
    "cronmower": "cronmower -v",
    "db:mem": "surreal start memory -A --auth --user root --pass root",
    "db": "surreal start file://./.surreal -A --auth --user root --pass root",
    "files": "npm run -s minio",
    "cron": "npm run -s cronmower -v",
    "backend": "npm run -s db & npm run -s files & npm run -s cron && fg",
    "backend-no-surreal": "npm run -s iles & npm run -s cron && fg",
    "db:reset": "rm -rf ./.minio",
    "file:reset": "rm -rf ./.surreal",
    "all:reset": "npm run -s db:reset && npm run -s file:reset",
    "frontend": "npm run -s build && npm start"
  },
  "browser": {
    "child_process": false
  },
  "dependencies": {
    "@langchain/anthropic": "^0.2.3",
    "@langchain/community": "^0.2.11",
    "@langchain/core": "^0.2.6",
    "@langchain/groq": "^0.0.13",
    "@langchain/openai": "^0.2.1",
    "compromise": "^14.13.0",
    "compromise-paragraphs": "^0.1.0",
    "cronmower": "^0.0.3",
    "dotenv": "^16.4.5",
    "draft-js": "^0.11.7",
    "exifr": "^7.1.3",
    "fork-async-iterator": "^0.0.0",
    "gpt-3-encoder": "^1.1.4",
    "identicon.js": "^2.3.3",
    "isolated-vm": "^5.0.1",
    "javascript-time-ago": "^2.5.10",
    "js-tiktoken": "^1.0.12",
    "markdown-it": "^14.1.0",
    "minio": "^8.0.1",
    "next": "15.0.0-rc.0",
    "obfo": "^0.0.9",
    "ollama": "^0.5.6",
    "pdf-parse-new": "^1.1.3",
    "react": "19.0.0-rc-fb9a90fa48-20240614",
    "react-dom": "19.0.0-rc-fb9a90fa48-20240614",
    "react-icons": "^5.2.1",
    "sanitize-html": "^2.13.0",
    "server-only": "^0.0.1",
    "surrealdb.js": "^1.0.0-beta.9",
    "yargs-parser": "^21.1.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.1"
  },
  "devDependencies": {
    "@types/identicon.js": "^2.3.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sanitize-html": "^2.11.0",
    "@types/yargs-parser": "^21.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.0-rc.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}